    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <userSettings>
    <Ghostice.ApplicationKit.Server.Console.Properties.Settings>
      <setting name="AppKitStartupTimeoutSeconds" serializeAs="String">
